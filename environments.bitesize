project: sltestnew
environments:
  - name: Development
    namespace: sltestnew-dev
    deployment:
      method: rolling-upgrade
    services:
      - name: node-app
        external_url: lmk-sltestnew.dev-bite.io
        port: 80
        ssl: "false"
        replicas: 1
        volumes:
          - name: nfs-sample-dev-data
            path: /data
            modes: ReadWriteMany
            size: 10G
